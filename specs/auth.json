{
  "openapi": "3.0.3",
  "info": {
    "title": "Auth API",
    "version": "1.0.0",
    "description": "Key management and access control."
  },
  "paths": {
    "/keys": {
      "get": {
        "operationId": "listKeys",
        "summary": "List API keys",
        "description": "Returns all active API keys.",
        "tags": ["auth"],
        "responses": {
          "200": {"description": "A list of keys"}
        }
      }
    },
    "/keys/{keyId}/rotate": {
      "post": {
        "operationId": "rotateKey",
        "summary": "Rotate an API key",
        "description": "Rotates an API key and returns the new value.",
        "tags": ["auth"],
        "parameters": [
          {
            "name": "keyId",
            "in": "path",
            "required": true,
            "schema": {"type": "string"}
          }
        ],
        "responses": {
          "200": {"description": "The rotated key"}
        }
      }
    }
  }
}
